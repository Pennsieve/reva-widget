{
  "name": "reva-widget",
  "version": "1.0.4",
  "description": "A Vue 3 component used for exploring the mapping between REVA 3D Vagus nerve tracing files and their corresponding MicroCT files",
  "private": false,
  "type": "module",
  "files": [
    "dist/*"
  ],
  "main": "dist/reva-widget.umd.cjs",
  "module": "dist/reva-widget.es.js",
  "exports": {
    ".": {
      "css": "./dist/reva-widget.css",
      "import": "./dist/reva-widget.es.js",
      "require": "./dist/reva-widget.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "ramda": "^0.30.1",
    "three": "^0.171.0",
    "three-orbitcontrols": "^2.110.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "element-plus": "^2.9.1",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue": "^3.5.13"
  },
  "peerDependencies": {
    "element-plus": "^2.9.1",
    "vue": "^3.5.13"
  },
  "author": "Eric Gauzens",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pennsieve/reva-widget.git"
  }
}
